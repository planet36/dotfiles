# pylint: disable=missing-module-docstring

import sys

# https://en.wikipedia.org/wiki/Wind_direction

# https://en.wikipedia.org/wiki/Cardinal_direction
card_dir = ('N', 'NE', 'E', 'SE', 'S', 'SW', 'W', 'NW')

degrees_to_cardinal = {
		0   : 'N',
		45  : 'NE',
		90  : 'E',
		135 : 'SE',
		180 : 'S',
		225 : 'SW',
		270 : 'W',
		315 : 'NW',
		360 : 'N',
		}

# https://www.unicode.org/charts/PDF/U1F800.pdf
arrows = ('ğŸ¢ƒ', 'ğŸ¢‡', 'ğŸ¢€', 'ğŸ¢„', 'ğŸ¢', 'ğŸ¢…', 'ğŸ¢‚', 'ğŸ¢†')

cardinal_to_arrow = {
		'N'  : 'ğŸ¢ƒ',
		'NE' : 'ğŸ¢‡',
		'E'  : 'ğŸ¢€',
		'SE' : 'ğŸ¢„',
		'S'  : 'ğŸ¢',
		'SW' : 'ğŸ¢…',
		'W'  : 'ğŸ¢‚',
		'NW' : 'ğŸ¢†',
		}

def closest(x, l):
	'''Find the closest value to x in l.'''
	return min(l, key=lambda y:abs(y - x))

for line in sys.stdin:
	wind_dir_deg = closest(int(line) % 360, degrees_to_cardinal.keys())
	wind_dir_card = degrees_to_cardinal[wind_dir_deg]
	print(cardinal_to_arrow[wind_dir_card])
