# pylint: disable=missing-module-docstring

__author__ = 'Steven Ward'
__license__ = 'OSL-3.0'
__version__ = '2024-12-30'

import os
import string
import sys


alphabet_set = frozenset(string.ascii_lowercase)


def is_pangram(s: str) -> bool:
    '''Determine if \a s is a pangram.'''
    return alphabet_set.issubset(s.lower())


# pylint: disable=missing-function-docstring
def main():

    for line in sys.stdin:
        line = line.rstrip(os.linesep)

        if not line: # empty
            continue

        # Use conversion flag '!a' when printing the input string.
        print(f'{is_pangram(line)}\t{line!a}')


if __name__ == '__main__':
    main()
